<resources>
    <declare-styleable name="RoundView">
        <attr name="titleName" format="string" />
        <attr name="titleSize" format="dimension" />
        <attr name="titleColor" format="color" />
        <attr name="roundBackground" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="TopTabView">
        <attr name="menuTextSize" format="dimension" />
        <attr name="menuSelectIcon" format="reference" />
        <attr name="menuUnSelectIcon" format="reference" />
        <attr name="tabWidth" format="dimension" />
    </declare-styleable>

</resources>
